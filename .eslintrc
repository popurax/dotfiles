{
  "env": {
    "es6": true,
    "node": true
  },
  "extends": ["standard"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": ["plugin:prettier/recommended"],
  "rules": {
    "func-call-spacing": [0],
    "new-cap": [0],
    "comma-dangle": [0]
  }
}
