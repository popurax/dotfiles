	"独自コンソールロガー": {
		"prefix": "log",
		"body": [
			"let logObjs = {\"label\": {avg:0,count:0}};",
			"const logTimeAvg = (label) => {",
			"const start = Date.now()",
			"return (logger = console.log) => {",
			"const n = Date.now() - start;",
			"const logObj = logObjs[label]||{}",
			"let { avg=0, count=0 } = logObj;",
			"avg=(avg+n)/2;",
			"count++;",
			"logger(label, {avg,count})",
			"logObjs[label] = {avg,count}",
			"}}",
		]
	}
